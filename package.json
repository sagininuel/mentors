{
    "name": "mentors",
    "version": "1.0.0",
    "description": "free social service",
    "main": "index.js",
    "scripts": {
        "test": "mocha ./api/test --require babel-register --recursive --timeout 50000 --exit",
        "lint": "./node_modules/.bin/eslint ./api --fix",
        "drop:create": "npm-run-all drop migrate",
        "coverage": "cross-env NODE_ENV=test npm run nyc -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
        "dev": "node_modules/.bin/nodemon ./api/src/index.js --exec babel-node --",
        "start": "babel-node -- ./api/src/index.js  ",
        "nyc": "nyc --reporter=lcov --reporter=text mocha 'api/test/'  --require babel-core/register --recursive --timeout 50000 --exit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sagininuel/mentors.git"
    },
    "keywords": [
        "mentor",
        "free"
    ],
    "author": "Sagini Emmanuel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sagininuel/mentors/issues"
    },
    "homepage": "https://github.com/sagininuel/mentors#readme",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "body-parser": "^1.19.0",
        "chai": "^4.2.0",
        "cross-env": "^5.2.0",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "express-validator": "^6.1.1",
        "jsonwebtoken": "^8.5.1",
        "supertest": "^4.0.2",
        "swagger-jsdoc": "^3.4.0",
        "swagger-ui-express": "^4.0.7"
    },
    "devDependencies": {
        "coveralls": "^3.0.6",
        "eslint": "^6.2.1",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.18.2",
        "mocha": "^6.2.0",
        "mocha-lcov-reporter": "^1.3.0",
        "nodemon": "^1.19.1",
        "nyc": "^14.1.1"
    }
}